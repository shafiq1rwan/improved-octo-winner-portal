	<div class="modal-dialog modal-dialog-centered" role="document">
		<div class="modal-content">
			<div class="modal-body text-center">
				<i class="fa fa-spinner fa-spin" style="font-size: 24px"></i> <br>
				<span>{{loadingText}}</span>
